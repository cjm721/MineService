<Window x:Class="WPFMineServiceTest.Window1"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFMineServiceTest"
        mc:Ignorable="d"
        Title="MineService Login" Height="313.107" Width="300">
    <Grid>
        <TextBox x:Name="textbox_username" HorizontalAlignment="Center" Height="23" Margin="0,148,0,0" TextWrapping="Wrap" Text="Username" VerticalAlignment="Top" Width="193"/>
        <ComboBox x:Name="combobox_cluster_select" HorizontalAlignment="Center" Margin="0,113,0,0" VerticalAlignment="Top" Width="193" IsEditable="True" Height="23"/>
        <Image x:Name="image_MS_logo" HorizontalAlignment="Center" Margin="0,5,0,0" VerticalAlignment="Top" Width="150" Source="pack://siteoforigin:,,,/Resources/Small Logo.png"/>
        <Button x:Name="button_add_cluster" Content="+" HorizontalAlignment="Left" VerticalAlignment="Top" Width="26" Margin="254,114,0,0"/>
        <PasswordBox x:Name="passwordbox_password" HorizontalAlignment="Center" Margin="0,184,0,0" VerticalAlignment="Top" Width="193" Height="23"/>
        <CheckBox x:Name="checkbox_remember_login" Content="Remember Login" HorizontalAlignment="Center" Margin="0,212,0,0" VerticalAlignment="Top"/>
        <Button x:Name="button_cancel" Content="Cancel" HorizontalAlignment="Left" Margin="52,242,0,0" VerticalAlignment="Top" Width="80" Height="37"/>
        <Button x:Name="button_login" Content="Login" HorizontalAlignment="Left" Margin="163,242,0,0" VerticalAlignment="Top" Width="80" Height="37"/>

    </Grid>
</Window>
